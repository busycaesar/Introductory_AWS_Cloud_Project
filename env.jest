# env.jest

# This file contains all the environment variables for running the tests.

# HTTP Port
PORT=8080

# Disabling the logs in tests.
# This can be changed to 'debug' if more detail is needed!
LOG_LEVEL=silent

# This is the location of the .htpasswd file containing the credentails useful while running tests!
HTPASSWD_FILE=tests/.htpasswd

AWS_COGNITO_POOL_ID=
AWS_COGNITO_CLIENT_ID=

API_URL=http://localhost:8080
